<div class="row">
    <div class="col-lg-12 mb-4">
        <div class="card box border-radius-none">
            <div class="card-body" data-bind="with:model">
                @*pagging*@
                <div class="col-12 align-right p-0 mb-3">
                    <span class="mr-2" data-bind="text:data.counterUnused() +' of '+data.totalUnused() "></span>
                    <div class="btn-group btn-group-toggle" data-toggle="buttons">
                        <label class="btn btn-sm btn-outline-secondary" data-bind="click:model.action.prevUnusedPage">
                            <input type="radio" name="options" id="prev-unused" autocomplete="off" checked>
                            <i class="mdi mdi-chevron-left"></i>
                        </label>
                        <label class="btn btn-sm btn-outline-secondary" data-bind="click:model.action.nextUnusedPage">
                            <input type="radio" name="options" id="next-unused" autocomplete="off"> <i class="mdi mdi-chevron-right"></i>
                        </label>
                    </div>
                </div>
                @*pagging*@
                @*start*@
                <ul class="list-group">
                    <li class="list-group-item mb-1 border-radius-none p-2">
                        <div class="p-0 col-12 d-sm-flex">
                            <div class="d-flex w-100">
                                <div class="bg-success align-center image-box-voucher">
                                    <div class="mdi mdi-silverware-fork-knife image-voucher"></div>
                                </div>
                                <div class="ml-3 w-100">
                                    <small>VC-052019-0023</small>
                                    <br />
                                    <bold>Exp 25 Januari 2020</bold>
                                </div>
                            </div>
                            <div class="w-100 p-sm-4">
                                <button class="btn btn-xs-sm btn-success float-right">Use</button>
                            </div>
                        </div>
                    </li>
                </ul>
                @*end*@
            </div>
        </div>
    </div>
</div>