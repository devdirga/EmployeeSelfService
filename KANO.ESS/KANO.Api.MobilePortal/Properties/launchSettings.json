{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:57722",
      "sslPort": 44391
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchUrl": "api/mobileportal",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "KANO.Api.MobilePortal": {
      "commandName": "Project",
      "launchUrl": "api/mobileportal",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:7034;http://localhost:7033"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/mobileportal",
      //"publishAllPorts": true,
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:7033;http://+:7034",
        "ASPNETCORE_HTTPS_PORT": "7033"
      },
      "httpPort": 7034,
      "useSSL": true,
      "sslPort": 7033
    }
  }
}