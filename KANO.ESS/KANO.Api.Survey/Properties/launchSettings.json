{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:7019",
      "sslPort": 7020
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": false,
      "launchUrl": "api/survey",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "KANO.Api.Survey": {
      "commandName": "Project",
      "launchBrowser": false,
      "launchUrl": "api/survey",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:7020;http://localhost:7019"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": false,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/survey",
      "publishAllPorts": true,
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:7020;http://+:7019",
        "ASPNETCORE_HTTPS_PORT": "7020"
      },
      "httpPort": 7019,
      "useSSL": true,
      "sslPort": 7020
    }
  }
}