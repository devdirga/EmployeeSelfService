{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:8070",
      "sslPort": 8073
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchUrl": "api/common",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "KANO.Api.Common": {
      "commandName": "Project",
      "launchUrl": "api/common",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:8073;http://localhost:8070"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": false,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/common",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:8073;http://+:8070",
        "ASPNETCORE_HTTPS_PORT": "8073"
      },
      "httpPort": 8070,
      "useSSL": true,
      "sslPort": 8073
    }
  }
}