{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:8060",
      "sslPort": 8063
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchUrl": "api/job",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "KANO.Api.BatchJob": {
      "commandName": "Project",
      "launchUrl": "api/job",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:8063;http://localhost:8060"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": false,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/job",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:8063;http://+:8060",
        "ASPNETCORE_HTTPS_PORT": "8063"
      },
      "httpPort": 8060,
      "useSSL": true,
      "sslPort": 8063
    }
  }
}