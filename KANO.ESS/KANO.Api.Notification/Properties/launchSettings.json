{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:8090",
      "sslPort": 8093
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/notification",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "KANO.Api.Notification": {
      "commandName": "Project",
      "launchUrl": "api/notification",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:8093;http://localhost:8090"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/notification",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:8093;http://+:8090",
        "ASPNETCORE_HTTPS_PORT": "8093"
      },
      "httpPort": 8090,
      "useSSL": true,
      "sslPort": 8093
    }
  }
}