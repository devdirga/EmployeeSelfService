{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:7001",
      "sslPort": 7002
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchUrl": "api/employee",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "KANO.Api.Employee": {
      "commandName": "Project",
      "launchUrl": "api/employee",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:7002;http://localhost:7001"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/employee",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:7002;http://+:7001",
        "ASPNETCORE_HTTPS_PORT": "7002"
      },
      "httpPort": 7001,
      "useSSL": true,
      "sslPort": 7002
    }
  }
}