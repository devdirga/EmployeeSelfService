{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:7003",
      "sslPort": 7004
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": false,
      "launchUrl": "api/benefit",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "KANO.Api.Benefit": {
      "commandName": "Project",
      "launchBrowser": false,
      "launchUrl": "api/benefit",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "https://localhost:7004;http://localhost:7003"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": false,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/benefit",
      "environmentVariables": {
        "ASPNETCORE_URLS": "https://+:7004;http://+:7003",
        "ASPNETCORE_HTTPS_PORT": "7004"
      },
      "httpPort": 7003,
      "useSSL": true,
      "sslPort": 7004
    }
  }
}